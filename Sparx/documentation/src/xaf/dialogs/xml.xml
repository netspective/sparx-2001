	<channel xsdn-element="/xaf/dialogs/dialog"/>
	<p/>
	<channel xsdn-element="/xaf/dialogs/dialog/client-js"/>
	<p/>
	<channel xsdn-element="/xaf/dialogs/dialog/director"/>
	<p/>
	<channel xsdn-element="/xaf/dialogs/dialog/execute-tasks"/>
	<p/>
	<channel xsdn-element="/xaf/dialogs/dialog/populate-tasks"/>
</page>