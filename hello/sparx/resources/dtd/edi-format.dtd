<!ATTLIST data-elem-group name CDATA #REQUIRED>
<!ATTLIST data-elem-group description CDATA #IMPLIED>
<!ATTLIST data-elem-group repeat-min CDATA '0'>
<!ATTLIST data-elem-group repeat-max CDATA '1'>
<!ATTLIST data-elem-group delimiter CDATA #IMPLIED>
<!ATTLIST data-elem-group hide-element CDATA #IMPLIED>
<!ELEMENT data-elem EMPTY>
<!ATTLIST data-elem name CDATA #REQUIRED>
<!ATTLIST data-elem description CDATA #IMPLIED>
<!ATTLIST data-elem type CDATA #IMPLIED>
<!ATTLIST data-elem repeat-min CDATA '0'>
<!ATTLIST data-elem repeat-max CDATA '1'>
<!ATTLIST data-elem length-min CDATA '0'>
<!ATTLIST data-elem length-max CDATA '0'>
<!ATTLIST data-elem delimiter CDATA #IMPLIED>
<!ATTLIST data-elem default CDATA #IMPLIED>
<!ATTLIST data-elem hide-element CDATA #IMPLIED>
