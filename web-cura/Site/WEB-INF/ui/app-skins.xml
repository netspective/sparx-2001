        </frame-head-font-attrs>
        <caption-cell-attrs>
            align='right'  valign='top' style='background: #0000A0'
        </caption-cell-attrs>
        <caption-font-attrs>
            face='Trebuchet MS, Arial' style='font-size: 11px'
        </caption-font-attrs>
        <separator-font-attrs>
            face='Trebuchet MS, Arial' color='#000000' style='font-size: 12px'
        </separator-font-attrs>
        <control-area-font-attrs>
            face='Trebuchet MS,Arial' style='font-size: 10px'
        </control-area-font-attrs>
        <control-area-style-attrs>
            style='font-size: 10px; background-color: #F4FAF1'
        </control-area-style-attrs>
        <hint-font-attrs>
            color='navy' face='Arial' style='font-size: 10px'
        </hint-font-attrs>
        <field-row-error-attrs>
