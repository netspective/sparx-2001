<?xml version="1.0"?>

<schema name="person">
	<table name="Gender" type="Enumeration">
		<enum>Male</enum>
		<enum>Female</enum>
		<enum>Not applicable</enum>
	</table>

	<table name="Marital_Status" abbrev="MarStat" type="Enumeration">
		<enum>Unknown</enum>
		<enum>Single</enum>
		<enum>Married</enum>
		<enum>Partner</enum>
		<enum>Legally Separated</enum>
		<enum>Divorced</enum>
		<enum>Widowed</enum>
		<enum>Not applicable</enum>
	</table>
</schema>